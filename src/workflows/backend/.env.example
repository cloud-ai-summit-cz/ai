# Copy to .env and fill with your Azure AI Foundry project endpoint
AZURE_AI_ENDPOINT=https://ai-foundry-<region>.services.ai.azure.com/api/projects/<project-name>

# Optional: user-assigned managed identity (UAMI)
# AZURE_CLIENT_ID=<uami-client-id>
# USER_ASSIGNED_IDENTITY_ID=/subscriptions/<sub>/resourceGroups/<rg>/providers/Microsoft.ManagedIdentity/userAssignedIdentities/<uami-name>
# AZURE_TENANT_ID=<tenant-id-if-different>
